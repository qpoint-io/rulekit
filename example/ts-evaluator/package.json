{
  "name": "@qpoint/rule-evaluator",
  "version": "0.1.0",
  "description": "TypeScript rule evaluator for rulekit AST - works in browser and Node.js",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "test": "tsx src/test-stringify.ts",
    "example": "tsx src/example.ts",
    "demo": "tsx src/demo.ts",
    "roundtrip": "tsx src/roundtrip.ts"
  },
  "keywords": ["rules", "ast", "evaluator", "browser", "vue", "react", "typescript"],
  "author": "Qpoint",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.3.0"
  }
}

